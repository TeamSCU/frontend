page {
  width: 100%;
  height: 100%;
  background: #fff;
}
.list{
  display:flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: flex-start;
  align-items: center;
  padding: 0 20rpx;
  margin-bottom: 50rpx;
}
.list .poi{
  position: relative;
  margin-bottom: 12rpx;
}
.ph{ 
  width: 349rpx;
  height: 349rpx;
  border-radius: 8rpx;
  vertical-align: bottom;
}
.time{ 
  position: absolute;
  left: 25rpx;
  color: #5c5c5c;
  font-size: 30rpx;
  font-weight: bold;
}